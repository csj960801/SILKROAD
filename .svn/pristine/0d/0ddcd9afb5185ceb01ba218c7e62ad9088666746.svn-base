<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.silkroad.SilkRoadDAO">

	<insert id="BoardInquiry" parameterType="com.silkroad.SilkRoadVO.SilkRoadVO">
		insert into Board(BoardTitle,BoardWriter, BoardContent)
		values(#{clientEmail},#{clientName},#{clientInquiry})
	</insert>

</mapper> 